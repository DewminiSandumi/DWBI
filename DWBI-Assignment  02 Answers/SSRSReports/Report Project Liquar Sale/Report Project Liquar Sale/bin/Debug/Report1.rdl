  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT f.DateCode, i.description, s.[Store Name], f.[Sale (Dollars)], s.City, i.Category
FROM     [Fact Sales] AS f INNER JOIN
                  dim_Store AS s ON s.[Store Number] = f.[Store Number] INNER JOIN
                  dim_Date AS d ON d.DateKey = f.DateCode INNER JOIN
                  dim_item AS i ON i.item_no_SK = f.item_no_SK
WHERE  (f.[Sale (Dollars)] &gt; 5000)</CommandText>
      </Query>
      <Fields>
