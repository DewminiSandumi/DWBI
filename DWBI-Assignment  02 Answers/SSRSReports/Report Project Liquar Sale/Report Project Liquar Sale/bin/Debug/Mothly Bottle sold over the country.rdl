  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select s.City,f.[ Bottles Sold],d.MonthYear,i.description,i.Category,s.[Country Name]
from dbo.[Fact Sales] f
inner join dbo.dim_Store s
on s.[Store Number] =f.[Store Number]
inner join dbo.dim_Date d
on d.DateKey=f.DateCode
inner join dbo.dim_item i
on i.item_no_SK=f.item_no_SK</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
